{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "check": {
      "dependsOn": ["^check"]
    },
    "test": {
      "inputs": ["$TURBO_DEFAULT$", "src/**/*.test.ts", "src/**/*.test.tsx"]
    },
    "lint": {},
    "typecheck": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "css:types:dev": {
      "cache": false,
      "persistent": true
    }
  }
}
